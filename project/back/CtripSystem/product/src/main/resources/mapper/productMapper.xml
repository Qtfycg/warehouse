<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qtfycg.product.mapper.productMapper">

    <resultMap id="BaseResultMap" type="com.qtfycg.product.domain.entity.product">
            <id property="id" column="id" />
            <result property="name" column="name" />
            <result property="price" column="price" />
            <result property="stock" column="stock" />
            <result property="description" column="description" />
            <result property="categoryId" column="type" />
            <result property="status" column="status" />
            <result property="imageList" column="image_list" />
    </resultMap>

    <sql id="Base_Column_List">
        id,name,price,stock,description,category_id,
        status,image_list
    </sql>
</mapper>
